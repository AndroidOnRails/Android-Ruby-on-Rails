<h1>Listing comment2s</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Comment</th>
    <th>User</th>
    <th>Photo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @comment2s.each do |comment2| %>
  <tr>
    <td><%= comment2.title %></td>
    <td><%= comment2.comment %></td>
    <td><%= comment2.user_id %></td>
    <td><%= comment2.photo_id %></td>
    <td><%= link_to 'Show', comment2 %></td>
    <td><%= link_to 'Edit', edit_comment2_path(comment2) %></td>
    <td><%= link_to 'Destroy', comment2, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Comment2', new_comment2_path %>
